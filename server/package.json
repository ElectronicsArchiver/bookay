  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bookmarks-parser": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-sequelize-crud": "^5.1.0",
    "heroku-ssl-redirect": "0.0.4",
    "pg": "^8.2.1",
    "sequelize": "^5.21.8",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "sqlite3": "^4.2.0",
